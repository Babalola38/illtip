.searchWrapper{
    display: flex;
    padding: 0 15px 8px;
    margin-top: 10px;
    .search_area{
        background-color: #ededed;
       
        //border-radius: 10px;
        text-align: center;
        position: relative;
        display: flex;
        margin-right: 10px;
        width: 100%;
        .serachDiv{
           
            background-color: rgba(0,0,0,0.05);
            height: 50px;
            border-radius: 6px;
            position: relative;
            width: 100%;
            font-size: 12px;
            color: #707070;
            text-align: left;
            padding-left: 21px;
            line-height: 52px;
            &:before{
                content: "";
                position: absolute;
                width: 15px;
                height: 16px;
                background-image: url(../../assets/imgs/searchIcon.png);
                background-repeat: no-repeat;
                background-position: center;
                background-size: 100%;
                right: 18px;
                top: 18px;
            }
            
        }
       
    }
    button.watchIcon{
        width: 60px;
        height: 50px;
        border-radius: 6px;
        background-color: #01135F;
        background-image: url(../../assets/imgs/clockIcon.png);
        background-position: center;
        background-repeat: no-repeat;
        background-size: 40%;
    }
}



.search_wrapper{
    padding: 16px;
}

.galleryImage{
    // flex-basis: 32%;
    //  margin-right: 2%;
    //  background-size: cover;
    //  background-repeat: no-repeat;
    //  height: 90px;
    //  margin-bottom: 7px;
    width: 100%;
    height: 120px;
    background-size: cover;
    background-position: top center;
    background-repeat: no-repeat;
    position: relative;
    border-radius: 6px;
    -webkit-box-shadow: 0px 0px 5px 1px rgba(212,175,55,1);
-moz-box-shadow: 0px 0px 5px 1px rgba(212,175,55,1);
box-shadow: 0px 0px 5px 1px rgba(212,175,55,1);
overflow: hidden;
video {
    position: absolute;
    top: 50%;
    left: 50%;
    min-width: 100%;
    min-height: 100%;
    width: auto;
    height: 250%;
    z-index: -100;
    transform: translateX(-50%) translateY(-50%);
}
 }
