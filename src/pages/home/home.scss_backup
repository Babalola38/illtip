//gallery
.gallery{
    height: auto;
    overflow-y: scroll;
    padding-bottom: 50px;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    padding: 16px;
    padding-top: 10px;
   //margin-top:210px;
    ul{
        margin: 0;
        padding: 0;
        list-style: none;
        display: flex;
        display: -webkit-flex;
    
     
     li:nth-child(3n+3){
         margin-right: 0;
     }
    }

    //padding: 0!important;
    }

    
    .listView{
        padding: 16px 16px 100px 16px;
    }
    .lftImg{
        flex-basis: 28%;
        background-size: cover;
        background-position: center;
        width: 28%;
        height: 76px;
    }
    .innerContent{
        flex-basis: 72%;
        padding-right: 10px;
        width: 72%;
    }
    ul.serviceAvailable{
        margin: 0;
        list-style: none;
        padding: 0;
        padding-top: 25px;
        li{
            display: flex;
            display: -webkit-flex;
            margin-bottom: 15px;
            min-height: 87px;
            background-color: #e0e0e0;

        }
        .liBd{
            flex-basis: 80%;
            width: 80%;
            position: relative;
            // padding-left: 85px;
            background-color: #e0e0e0;
            border-top-left-radius: 5px;
            border-bottom-left-radius: 5px;
            display: flex;
            padding-top: 7px;
            padding-left: 7px;
            padding-bottom: 7px;
            h5{
                margin: 0;
                color: #252525;
                font-family: 'Avenir-Roman';
                font-size: 14px;
                text-transform: uppercase;
                margin-bottom: 5px;
                margin-top: 0;
                text-overflow: ellipsis;
                white-space: nowrap;
                overflow: hidden;
            }
            p{
                margin: 0;
                font-size: 12px;
                color: #252525;
                font-family: 'Avenir-Roman';
                width: 97%;
                text-overflow: ellipsis;
                overflow: hidden;
                white-space: nowrap;
            }
        }
        .rgtSection{
            flex-basis: 20%;
            width: 20%;
            background: #d4d4d4;
            border-top-right-radius: 5px;
            border-bottom-right-radius: 5px;
            text-align: center;
            padding-top: 5px;
            span{
                display: block;
                line-height: 16px;
                margin-bottom: 3px;
                font-size: 14px;
                color: #252525;
                font-family: 'Avenir-Roman';
            }
            .location{
                width: 16px;
                margin: 0 auto;
            }
        }
        .urgent{
            background:-webkit-linear-gradient(180deg, rgb(220, 4, 45) 0%, rgb(72, 24, 113) 80%);
            background:-o-linear-gradient(180deg, rgb(220, 4, 45) 0%, rgb(72, 24, 113) 80%);
            background:-moz-linear-gradient(180deg, rgb(220, 4, 45) 0%, rgb(72, 24, 113) 80%);
            background:linear-gradient(180deg, rgb(220, 4, 45) 0%, rgb(72, 24, 113) 80%);
            padding: 2px;
            border-radius: 5px;
        }
        .innerContent{
            padding-left: 7px;
        }
    }
.star{
    display: inline-block;
    width: 70px;
    margin-right: 5px;
}
.totalReview{
    font-size: 11px;
    color: #252525;
    font-family: 'Avenir-Roman';
}
.conFirm{
    background-color: #002b57;
