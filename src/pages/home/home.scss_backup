  .isYellow::before{
    content: '';
    position: absolute;
    z-index: -1;
    width: 60px;
    height: 60px;
    display: block;
    // margin: -2px;
    top: -2.5px;
    right: -3px;
    background: linear-gradient(to right, #f6f6f6 0%, #f6f6f6 50%, #efde34 51%, #efde34 100%);
    border-radius: 50%;
  }
  .isGreen{
    border:3px solid green
}
